@article{rootpoly,
	abstract = {We study the root polytope {\$}{$\backslash$}mathcal{\{}P{\}}{\_}{\{}{$\backslash$}varPhi{\}}{\$}of a finite irreducible crystallographic root system Φ using its relation with the Abelian ideals of a Borel subalgebra of a simple Lie algebra with root system Φ. We determine the hyperplane arrangement corresponding to the faces of cxdimension 2 of {\$}{$\backslash$}mathcal{\{}P{\}}{\_}{\{}{$\backslash$}varPhi{\}}{\$}and analyze its relation with the facets of {\$}{$\backslash$}mathcal{\{}P{\}}{\_}{\{}{$\backslash$}varPhi{\}}{\$}. For Φof type Anor Cn, we show that the orbits of some special subsets of Abelian ideals under the action of the Weyl group parametrize a triangulation of {\$}{$\backslash$}mathcal{\{}P{\}}{\_}{\{}{$\backslash$}varPhi{\}}{\$}. We show that this triangulation restricts to a triangulation of the positive root polytope {\$}{$\backslash$}mathcal{\{}P{\}}{\_}{\{}{$\backslash$}varPhi{\}}\^{}{\{}+{\}}{\$}.},
	author = {Cellini, Paola and Marietti, Mario},
	date = {2014/05/01},
	date-added = {2024-12-30 20:56:07 -0500},
	date-modified = {2024-12-30 20:56:43 -0500},
	doi = {10.1007/s10801-013-0458-5},
	id = {Cellini2014},
	isbn = {1572-9192},
	journal = {Journal of Algebraic Combinatorics},
	number = {3},
	pages = {607--645},
	title = {Root polytopes and Abelian ideals},
	url = {https://doi.org/10.1007/s10801-013-0458-5},
	volume = {39},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.1007/s10801-013-0458-5}}


